<!-- OSSEC Decoder for Sysmon Event ID 1: Process Created
  -
  -Use the appropriate decoder for your version of Sysmon - v1 or v3
  _
  -  OSSEC to Sysmon Fields Mapping:
  -  user = User
  -  status = Image
  -  url = Hash
  -  extra_data = ParentImage
  -
  - Compatible with Server 2003/2008/2012
-->

<decoder name="Sysmon-EventID">
<type>windows</type>
<prematch>INFORMATION\(1\)</prematch>
<regex offset="after_prematch">Image: (\.*)  \s*CommandLine: \.* \s*User: (\.*) \s*LogonGuid: \S* \.*Hash\w*: (\S*) \s*ParentProcessGuid: \S* \s*ParentProcessID: \S* \s*ParentImage: (\.*) \.*</regex>
<order>status,user,url,data</order>
</decoder>
